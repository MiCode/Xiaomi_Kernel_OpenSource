	krait0_vreg: regulator@f9088000 {
		compatible = "qcom,krait-regulator";
		regulator-name = "krait0";
		reg = <0xf9088000 0x1000>;
		regulator-min-microvolt = <500000>;
		regulator-max-microvolt = <1100000>;
	};

