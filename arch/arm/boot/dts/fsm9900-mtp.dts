/dts-v1/;

/include/ "fsm9900.dtsi"

/ {
	model = "Qualcomm FSM9900 MTP";
	compatible = "qcom,fsm9900-mtp", "qcom,fsm9900", "qcom,mtp";
	qcom,msm-id = <188 8 0>;

	aliases {
		serial0 = &uart0;
	};
};

&soc {
	uart0: serial@f9960000 {
		status = "ok";
	};
};
