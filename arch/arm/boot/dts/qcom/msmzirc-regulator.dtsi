&rpm_bus {
	rpm-regulator-smpa1 {
		status = "okay";
		pmd9635_s1: regulator-s1 {
			regulator-min-microvolt = <700000>;
			regulator-max-microvolt = <1100000>;
			qcom,init-voltage = <700000>;
			status = "okay";
		};
	};

	rpm-regulator-smpa2 {
		status = "okay";
		pmd9635_s2: regulator-s2 {
			regulator-min-microvolt = <1250000>;
			regulator-max-microvolt = <1250000>;
			qcom,init-voltage = <1250000>;
			status = "okay";
		};
	};

	rpm-regulator-smpa3 {
		status = "okay";
		pmd9635_s3: regulator-s3 {
			regulator-min-microvolt = <1025000>;
			regulator-max-microvolt = <1025000>;
			qcom,init-voltage = <1025000>;
			status = "okay";
		};
	};

	rpm-regulator-smpa4 {
		status = "okay";
		pmd9635_s4: regulator-s4 {
			regulator-min-microvolt = <1850000>;
			regulator-max-microvolt = <1850000>;
			qcom,init-voltage = <1850000>;
			status = "okay";
		};
	};

	/* PMD9635 S5 = VDD_CX supply */
	rpm-regulator-smpa5 {
		status = "okay";
		pmd9635_s5_corner: regulator-s5-corner {
			compatible = "qcom,rpm-smd-regulator";
			regulator-name = "9635_s5_corner";
			qcom,set = <3>;
			regulator-min-microvolt = <1>;
			regulator-max-microvolt = <7>;
			qcom,use-voltage-corner;
		};

		pmd9635_s5_corner_ao: regulator-s5-corner-ao {
			compatible = "qcom,rpm-smd-regulator";
			regulator-name = "9635_s5_corner_ao";
			qcom,set = <1>;
			regulator-min-microvolt = <1>;
			regulator-max-microvolt = <7>;
			qcom,use-voltage-corner;
		};

		pmd9635_s5_floor_corner: regulator-s5-floor-corner {
			compatible = "qcom,rpm-smd-regulator";
			regulator-name = "9635_s5_floor_corner";
			qcom,set = <3>;
			regulator-min-microvolt = <1>;
			regulator-max-microvolt = <7>;
			qcom,use-voltage-floor-corner;
		};
	};

	rpm-regulator-ldoa1 {
		status = "okay";
		pmd9635_l1: regulator-l1 {
			regulator-min-microvolt = <1225000>;
			regulator-max-microvolt = <1250000>;
			qcom,init-voltage = <1225000>;
			status = "okay";
		};
	};

	rpm-regulator-ldoa2 {
