	};

	rpm-regulator-ldoa10 {
		status = "okay";
		pmd9635_l10: regulator-l10 {
			regulator-min-microvolt = <3200000>;
			regulator-max-microvolt = <3200000>;
			qcom,init-voltage = <3200000>;
