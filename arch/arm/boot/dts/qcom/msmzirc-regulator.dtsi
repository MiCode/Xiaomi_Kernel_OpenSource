			status = "okay";
		};
	};

	rpm-regulator-ldoa3 {
		status = "okay";
		pmd9635_l3: regulator-l3 {
			regulator-min-microvolt = <1000000>;
			regulator-max-microvolt = <1000000>;
			qcom,init-voltage = <1000000>;
			status = "okay";
		};
	};

	rpm-regulator-ldoa4 {
