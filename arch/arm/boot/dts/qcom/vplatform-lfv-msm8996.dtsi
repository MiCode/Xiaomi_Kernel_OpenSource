			"msm-dai-q6-tdm.36868", "msm-dai-q6-tdm.36870",
			"msm-dai-q6-tdm.36880", "msm-dai-q6-tdm.36882",
			"msm-dai-q6-tdm.36884", "msm-dai-q6-tdm.36886",
			"msm-dai-q6-tdm.36881", "msm-dai-q6-tdm.36883",
