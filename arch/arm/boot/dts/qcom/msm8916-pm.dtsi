			<0xff 275>, /* rpm_ipc(30) */
			<0xff 276>; /* rpm_ipc(31) */

		qcom,gpio-parent = <&msm_gpio>;
		qcom,gpio-map = <3  108 >,
			<4  1 >,
			<5  5 >,
			<6  9 >,
			<7  107>,
			<8  98>,
