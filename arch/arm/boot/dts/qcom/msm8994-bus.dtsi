			qcom,buswidth = <8>;
			qcom,ap-owned;
			qcom,connections = <&slv_bimc_snoc>;
			qcom,mas-rpm-id = <ICBID_MASTER_MSS_PROC>;
		};


		mas_oxili: mas-oxili {
			cell-id = <MSM_BUS_MASTER_GRAPHICS_3D>;
			label = "mas-oxili";
