			qcom,connections = <&mas_bimc_snoc>;
			qcom,slv-rpm-id = <ICBID_SLAVE_BIMC_SNOC>;
		};


		/* mnoc Devices */
		mas_cnoc_mnoc_mmss_cfg: mas-cnoc-mnoc-mmss-cfg {
