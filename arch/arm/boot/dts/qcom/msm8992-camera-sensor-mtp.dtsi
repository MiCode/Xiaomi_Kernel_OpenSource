	eeprom6: qcom,eeprom@6 {
		cell-index = <6>;
		reg = <6>;
		qcom,eeprom-name = "s5k3m2xm_ofilm";
		compatible = "qcom,eeprom";
		qcom,slave-addr = <0xa0>;
		qcom,cci-master = <0>;
