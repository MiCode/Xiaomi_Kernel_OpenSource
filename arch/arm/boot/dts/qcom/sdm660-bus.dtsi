				 &slv_ahb2phy &slv_blsp_2
				 &slv_blsp_1 &slv_pimem_cfg
				 &slv_glm &slv_message_ram
				 &slv_cnoc_a2noc &slv_bimc_cfg
				 &slv_cnoc_mnoc_cfg>;
