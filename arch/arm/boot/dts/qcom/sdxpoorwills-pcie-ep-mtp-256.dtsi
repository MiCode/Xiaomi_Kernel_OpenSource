#include "sdxpoorwills-mtp.dtsi"

&vbus_detect {
	status = "okay";
};

&usb {
	status = "okay";
	qcom,connector-type-uAB;
	extcon = <0>, <0>, <0>, <&vbus_detect>;
};

&pcie_ep {
	status = "okay";
};

&ipa_hw {
	qcom,use-ipa-in-mhi-mode;
};

&pcie0 {
	status = "disabled";
};

&mhi_device {
	status = "okay";
};
