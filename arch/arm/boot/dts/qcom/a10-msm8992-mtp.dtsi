		pericom@1d {
			compatible = "pericom,30216a";
			reg = <0x1d>;

			interrupt-parent = <&msm_gpio>;
			interrupts = <31 0>;

			i2c-supply = <&pm8994_lvs2>;
		};

		tusb320@60 {
			compatible = "ti,tusb320";
			reg = <0x60>;

			interrupt-parent = <&msm_gpio>;
			interrupts = <31 0>;

			i2c-supply = <&pm8994_lvs2>;
        };

		tusb320l@47 {
			compatible = "ti,tusb320l";
			reg = <0x47>;

			interrupt-parent = <&msm_gpio>;
			interrupts = <31 0>;

			i2c-supply = <&pm8994_lvs2>;
