&pm8916_mpps {
	mpp@a000 { /* MPP 1 */
		/* VDD_PX */
		status = "disabled";
	};

	mpp@a100 { /* MPP 2 */
		/* HR LED */
	};

	mpp@a200 { /* MPP 3 */
		/* VREF DAC */
		status = "disabled";
	};

	mpp@a300 { /* MPP 4 */
		/* Backlight PWM */
		qcom,mode = <1>;		/* Digital output */
		qcom,invert = <0>;		/* Disable invert */
		qcom,src-sel = <4>;		/* DTEST1 */
		qcom,vin-sel = <0>;		/* VPH_PWR */
		qcom,master-en = <1>;		/* Enable MPP */
	};
};

&pm8916_gpios {
	gpio@c000 { /* GPIO 1 */
		qcom,mode = <1>;		/* Digital output */
		qcom,pull = <4>;		/* Pull down */
		qcom,vin-sel = <2>;		/* VIN 2*/
		qcom,src-sel = <1>;		/* GPIO */
		qcom,master-en = <1>;	/* Enable GPIO */
		status = "okay";
	};

	gpio@c100 { /* GPIO 2 */
		qcom,mode = <1>;		/* Digital output */
		qcom,pull = <4>;		/* Pull down */
		qcom,vin-sel = <2>;		/* VIN 2*/
		qcom,src-sel = <1>;		/* GPIO */
		qcom,master-en = <1>;	/* Enable GPIO */
		status = "okay";
	};

	/* USB_HUB_RESET_N*/
	gpio@c200 { /* GPIO 3*/
		qcom,mode = <1>;	/* Digital output */
		qcom,pull = <5>;	/* No PULL */
		qcom,vin-sel = <0>;	/* VPH */
		qcom,src-sel = <0>;	/* CONSTANT */
		qcom,master-en = <1>; 	/* ENABLE GPIO */
		status = "okay";
	};

	/* USB_SW_SEL*/
	gpio@c300 { /* GPIO 4 */
		qcom,mode = <1>;	/* Digital output */
		qcom,pull = <5>;	/* No PULL */
		qcom,vin-sel = <0>;	/* VPH */
		qcom,src-sel = <0>;	/* CONSTANT */
		qcom,master-en = <1>; 	/* ENABLE GPIO */
		status = "okay";
	};
};

