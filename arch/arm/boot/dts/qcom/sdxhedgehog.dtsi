#include "mdmcalifornium.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. SDX HEDGEHOG";
	compatible = "qcom,sdxhedgehog";
	qcom,msm-id = <314 0x0>;

	timer {
		compatible = "arm,armv7-timer";
		interrupts = <1 13 0xf08>,
			<1 12 0xf08>,
			<1 10 0xf08>,
			<1 11 0xf08>;
		clock-frequency = <19200000>;
	};
};

