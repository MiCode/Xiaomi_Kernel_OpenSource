#include "msm8929-gpu.dtsi"
#include "msm8929-bus.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. MSM8929";
	compatible = "qcom,msm8929";
	qcom,msm-id =	<268 0>,
			<269 0>,
			<270 0>,
			<271 0>;
