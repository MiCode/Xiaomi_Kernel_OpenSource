			qcom,psci-mode-shift = <8>;
			qcom,psci-mode-mask = <0xf>;

			qcom,pm-cluster-level@0{
				reg = <0>;
				label = "system-wfi";
				qcom,psci-mode = <0x0>;
				qcom,latency-us = <100>;
				qcom,ss-power = <725>;
				qcom,energy-overhead = <85000>;
				qcom,time-overhead = <120>;
