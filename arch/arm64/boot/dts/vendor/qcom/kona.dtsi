		/* low: 18-60 Mbps, ddr freq: 451.2 MHz*/
		<MSM_BUS_MASTER_PCIE MSM_BUS_SLAVE_FIRST 7500 1600000>,
		/* medium: 60-240 Mbps, ddr freq: 451.2 MHz */
