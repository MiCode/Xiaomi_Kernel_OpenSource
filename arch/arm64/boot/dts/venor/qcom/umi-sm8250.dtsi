&tlmm {
	/* GPIO_24: FP_RESET_N */
	msm_gpio_24: msm_gpio_24 {
		mux {
			 pins = "gpio24";
			 function = "gpio";
		};
		config {
			pins = "gpio24";
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};
	/* GPIO_24 : FP_RESET_N, state device active*/
	msm_gpio_24_output_high: msm_gpio_24_output_high {
		mux {
			pins = "gpio24";
			function = "gpio";
		};
		config {
			pins = "gpio24";
			drive-strength = <2>;
			bias-disable;
			output-high;
		};
	};
	/* GPIO_23 : FP_INT_N */
	msm_gpio_23: msm_gpio_23 {
		mux {
			pins = "gpio23";
			function = "gpio";
		};
		config {
			pins = "gpio23";
			drive-strength = <2>;
			bias-pull-down;
		};
	};

	/* GPIO_25 : FP_12V_N */
	msm_gpio_25: msm_gpio_25 {
		mux {
			pins = "gpio25";
			function = "gpio";
		};
		config {
			pins = "gpio25";
			drive-strength = <2>;
			bias-pull-down;
		};
	};

	/* GPIO_26 : FP_18V_N */
	msm_gpio_26: msm_gpio_26 {
		mux {
			pins = "gpio26";
			function = "gpio";
		};
		config {
			pins = "gpio26";
			drive-strength = <2>;
			bias-pull-down;
		};
	};
};

