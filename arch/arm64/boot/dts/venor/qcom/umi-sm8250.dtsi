	goodix@5d {
		compatible = "goodix,gt9889";
		reg = <0x5d>;
		status = "ok";
		interrupt-parent = <&tlmm>;
		interrupts = <39 0x2800>;
		/*goodix,avdd_name = "vtouch";*/
