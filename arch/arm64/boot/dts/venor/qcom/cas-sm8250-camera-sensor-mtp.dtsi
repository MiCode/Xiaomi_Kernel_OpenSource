/*PM9008*/
		pm8008_interrupt: pm8008_interrupt {
			mux {
				pins = "gpio59";
				function = "gpio";
			};

			config {
				pins = "gpio59";
				bias-disable;
				input-enable;
			};
		};

		pm8008_active: pm8008_active {
			mux {
				pins = "gpio25";
				function = "gpio";
			};

			config {
				pins = "gpio25";
				bias-pull-up;
				output-high;
				drive-strength = <2>;
			};
		};

		ISL91107llNZ_MODE_active: ISL91107llNZ_MODE_active {
			mux {
				pins = "gpio139";
				function = "gpio";
			};

			config {
