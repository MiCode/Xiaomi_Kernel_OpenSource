	/* QUPv3 South_1 Instances
	 * South_1 0 : SE 6
	 * South_1 1 : SE 7
	 * South_1 2 : SE 8
	 * South_1 3 : SE 9
	 * South_1 4 : SE 10
	 * South_1 5 : SE 11
	 */
	qupv3_1: qcom,qupv3_1_geni_se@9c0000 {
		compatible = "qcom,qupv3-geni-se";
