			mt6363_vs2: VS2 {
				regulator-compatible = "VS2";
				regulator-name = "mt6363_vs2";
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1600000>;
				regulator-enable-ramp-delay = <180>;
				regulator-allowed-modes = <0 1 2>;
				regulator-always-on;
			};
			mt6363_vbuck1: VBUCK1 {
