	mtk_battery_oc_throttling {
		compatible = "mediatek,mt6357-battery_oc_throttling";
	};

	pmic_lbat_service {
		compatible = "mediatek,mt6357-lbat_service";
	};

	pmic_debug {
		compatible = "mediatek,mt6357-debug";
	};
};
