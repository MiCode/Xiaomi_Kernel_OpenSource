	};

	/* chosen */
	chosen: chosen {
		bootargs = "console=tty0 console=ttyS0,921600n1 root=/dev/ram \
			    loglevel=8 \
			    earlycon=mtk8250,mmio32,0x11001000 \
			    androidboot.selinux=permissive \
			    initcall_debug=1 \
			    vmalloc=400M swiotlb=noforce cma=64M \
			    firmware_class.path=/vendor/firmware";
		kaslr-seed = <0 0>;
	};

