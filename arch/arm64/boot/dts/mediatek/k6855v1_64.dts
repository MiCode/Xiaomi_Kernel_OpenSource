#include <dt-bindings/interrupt-controller/arm-gic.h>
#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/pinctrl/mt6855-pinfunc.h>

&chosen {
	bootargs = "console=tty0 console=ttyS0,921600n1 root=/dev/ram \
loglevel=8 \
androidboot.hardware=mt6855 \
earlycon=mtk8250,mmio32,0x11001000 \
vmalloc=400M swiotlb=noforce cma=64M \
firmware_class.path=/vendor/firmware \
initcall_debug=1";
	atag,videolfb-fb_base_l = <0x48700000>;
	atag,videolfb-fb_base_h = <0x0>;
	atag,videolfb-islcmfound = <1>;
	atag,videolfb-islcm_inited = <0>;
	atag,videolfb-fps= <6000>;
	atag,videolfb-vramSize= <0x1be0000>;
	atag,videolfb-lcmname=
		"nt35595_fhd_dsi_cmd_truly_nt50358_drv";
};

&timer {
	clock-frequency = <150000000>;
};


&mtk_leds {
	compatible = "mediatek,mtk-leds";

	backlight {
		led_mode = <6>;
		gate_enable = <1>;
		pwm_config = <0 1 0 0 0>;
	};
};

