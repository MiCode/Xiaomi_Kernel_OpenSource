			output-high;
		};
	};
	rcv_gpio_low: rcv_gpio_low {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO54__FUNC_GPIO54>;
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};
};

