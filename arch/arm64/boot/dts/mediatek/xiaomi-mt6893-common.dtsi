	};

	cs35l41_top: cs35l41@42 {
		compatible = "cirrus,cs35l41";
		#sound-dai-cells = <0>;
		reg = <0x42>;
		reset-gpio = <&pio 30 0>;
		interrupt-parent = <&pio>;
		interrupts = <113 IRQ_TYPE_LEVEL_LOW 113 0>;
		irq-gpio = <&pio 113 0>;
