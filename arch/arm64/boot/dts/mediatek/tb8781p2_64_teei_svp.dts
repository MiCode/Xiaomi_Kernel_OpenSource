&chosen {

};

&android {
	hardware = "mt8781";
};

&utos {
	microtrust,real-drv = <1>;
};

&mtk_leds {
	compatible = "mediatek,disp-leds";

	backlight {
		led_mode = <4>;
		pwm_config = <0 1 0 0 0>;
	};
};

&pio {
	mtkfb_pins_lcm_led_en1: lcm_led_en1_gpio {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO12__FUNC_GPIO12>;
			slew-rate = <1>;
			output-high;
		};
	};

	mtkfb_pins_lcm_led_en0: lcm_led_en0_gpio {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO12__FUNC_GPIO12>;
			slew-rate = <1>;
			output-low;
		};
	};

	mtkfb_pins_lcm_rst_out1_gpio: lcm_rst_out1_gpio {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO85__FUNC_GPIO85>;
			slew-rate = <1>;
			output-high;
		};
	};

	mtkfb_pins_lcm_rst_out0_gpio: lcm_rst_out0_gpio {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO85__FUNC_GPIO85>;
			slew-rate = <1>;
			output-low;
		};
	};
	mtkfb_pins_lcm_dsi_te: lcm_dsi_te {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO83__FUNC_DSI_TE>;
		};
	};

	i2c1_pin: i2c1_pin {
		pins_cmd0_dat {
			pinmux = <PINMUX_GPIO132__FUNC_SCL1>;
			bias-pull-up;
		};
		pins_cmd1_dat {
			pinmux = <PINMUX_GPIO133__FUNC_SDA1>;
			bias-pull-up;
		};
	};

	bq2589x_int: bq2589x_int_pin {
		pins_cmd0_dat {
			pinmux = <PINMUX_GPIO12__FUNC_GPIO12>;
			bias-pull-up;
			input-enable;
		};
	};
};

&charger {
	charger = <&bq2589x>;
	gauge = <&extern_gauge>;
	battery_cv = <4400000>;
};

&extcon_usb {
	vbus-supply = <&usb_otg_vbus>;
	vbus-voltage;
	vbus-current;
	charger = <&bq2589x>;
};

&i2c1 {
	status = "okay";
	pinctrl-names = "default", "default";
	pinctrl-0 = <&i2c1_pin>;
	pinctrl-1 = <&bq2589x_int>;

	bq2589x: bq2589x@6A {
		status = "okay";
		compatible = "ti,bq25890";
		reg = <0x6A>;
		charger_name = "primary_chg";
		interrupt-parent = <&pio>;
		interrupts = <12 IRQ_TYPE_EDGE_FALLING>;
		ti,irq-gpios = <&pio 12 0>;
		ti,chg-en-gpio = <&pio 80 0>;
		ti,otg-en-gpio = <&pio 95 0>;
		ti,otg-en2-gpio = <&pio 88 0>;
		ti,otg-sgm6111-gpio = <&pio 154 0>;
		ti,otg-ocflag-gpio = <&pio 89 0>;
		ti,bq2589x,charge-detect-enable;
		interrupt-names = "chr_stat";
		usb_otg_vbus: usb-otg-vbus {
			regulator-compatible = "usb-otg-vbus";
			regulator-name = "usb-otg-vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
		};
	};

	extern_gauge: extern_gauge@55 {
		compatible = "nvt,mm8013";
		reg = <0x55>;
		charger = <&bq2589x>;
	};
};

&mtk_gauge {
	status = "disable";
	DISABLE_MTKBATTERY;
	charger = <&bq2589x>;
	gauge = <&extern_gauge>;
};

&mtk_ctd {
	bc12 = <&bq2589x>;
	bc12_sel = <0>;
};

&mtkfb {
	pinctrl-names = "lcm_led_en1_gpio", "lcm_led_en0_gpio",
		"lcm_rst_out1_gpio", "lcm_rst_out0_gpio",
		"mode_te_te";
	pinctrl-0 = <&mtkfb_pins_lcm_led_en1>;
	pinctrl-1 = <&mtkfb_pins_lcm_led_en0>;
	pinctrl-2 = <&mtkfb_pins_lcm_rst_out1_gpio>;
	pinctrl-3 = <&mtkfb_pins_lcm_rst_out0_gpio>;
	pinctrl-4 = <&mtkfb_pins_lcm_dsi_te>;
	status = "okay";
};

&dispsys_config {
	pinctrl-names =
		"lcm_rst_out1_gpio", "lcm_rst_out0_gpio",
		"mode_te_te";
	pinctrl-0 = <&mtkfb_pins_lcm_rst_out1_gpio>;
	pinctrl-1 = <&mtkfb_pins_lcm_rst_out0_gpio>;
	pinctrl-2 = <&mtkfb_pins_lcm_dsi_te>;
	status = "okay";
};

&dsi0 {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
	panel1@0 {
		compatible = "boe,hx83102j,vdo,120hz";
		reg = <0>;
		pm-enable-gpios = <&pio 87 0>;
		reset-gpios = <&pio 85 0>;
		bias-gpios = <&pio 82 0>,
			<&pio 81 0>;
		pinctrl-names = "default";
		port {
			panel_in1: endpoint {
				remote-endpoint = <&dsi_out>;
			};
		};
	};
	panel2@0 {
		compatible = "tianma,hx83102j,vdo,120hz";
		reg = <1>;
		pm-enable-gpios = <&pio 87 0>;
		reset-gpios = <&pio 85 0>;
		bias-gpios = <&pio 82 0>,
			<&pio 81 0>;
		pinctrl-names = "default";
		port {
			panel_in2: endpoint {
				remote-endpoint = <&dsi_out>;
			};
		};
	};
	ports {
		port {
			dsi_out: endpoint {
				remote-endpoint = <&panel_in1>;
			};
		};
	};
};

/* GPS GPIO standardization start */
&pio {
	gps_pins_default: gps_default {
	};
	gps_l1_lna_pins_ol: gps_l1_lna@0 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO25__FUNC_GPIO25>;
			output-low;
		};
	};
	gps_l1_lna_pins_dsp_ctrl: gps_l1_lna@1 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO25__FUNC_GPS_L1_ELNA_EN>;
		};
	};
	gps_l1_lna_pins_oh: gps_l1_lna@2 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO25__FUNC_GPIO25>;
			output-high;
		};
	};
	gps_l5_lna_pins_ol: gps_l5_lna@0 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO26__FUNC_GPIO26>;
			output-low;
		};
	};
	gps_l5_lna_pins_dsp_ctrl: gps_l5_lna@1 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO26__FUNC_GPS_L5_ELNA_EN>;
		};
	};
	gps_l5_lna_pins_oh: gps_l5_lna@2 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO26__FUNC_GPIO26>;
			output-high;
		};
	};
};
&gps {
	pinctrl-names = "default",
		"gps_l1_lna_disable",
		"gps_l1_lna_dsp_ctrl",
		"gps_l1_lna_enable",
		"gps_l5_lna_disable",
		"gps_l5_lna_dsp_ctrl",
		"gps_l5_lna_enable";
	pinctrl-0 = <&gps_pins_default>;
	pinctrl-1 = <&gps_l1_lna_pins_ol>;
	pinctrl-2 = <&gps_l1_lna_pins_dsp_ctrl>;
	pinctrl-3 = <&gps_l1_lna_pins_oh>;
	pinctrl-4 = <&gps_l5_lna_pins_ol>;
	pinctrl-5 = <&gps_l5_lna_pins_dsp_ctrl>;
	pinctrl-6 = <&gps_l5_lna_pins_oh>;
