&soc {
	tlmm: pinctrl@03000000 {
		compatible = "qcom,sdm855-pinctrl";
		reg = <0x03000000 0xdc2000>;
		interrupts = <0 208 0>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
	};
};
