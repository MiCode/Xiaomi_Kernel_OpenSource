						drive-strength = <2>;
						bias-pull-down;
				};
			};
		};

		qupv3_se9_4uart_pins: qupv3_se9_4uart_pins {
			qupv3_se9_ctsrx: qupv3_se9_ctsrx {
				mux {
					pins = "gpio10", "gpio13";
					function = "qup14";
				};

				config {
					pins = "gpio10", "gpio13";
					drive-strength = <2>;
					bias-no-pull;
				};
			};

			qupv3_se9_rts: qupv3_se9_rts {
				mux {
					pins = "gpio11";
					function = "qup14";
				};

				config {
