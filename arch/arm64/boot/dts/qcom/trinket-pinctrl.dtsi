						bias-pull-up;
				};
			};
		};

		qupv3_se3_4uart_pins: qupv3_se3_4uart_pins {
			qupv3_se3_rx: qupv3_se3_rx {
				mux {
					pins = "gpio15";
					function = "qup03";
				};

				config {
					pins = "gpio15";
					drive-strength = <2>;
					bias-no-pull;
				};
			};

			qupv3_se3_tx: qupv3_se6_tx {
				mux {
					pins = "gpio14";
					function = "qup03";
				};

				config {
					pins = "gpio14";
					drive-strength = <2>;
					bias-pull-up;
				};
			};
		};

		qupv3_se4_2uart_pins: qupv3_se4_2uart_pins {
			qupv3_se4_2uart_active: qupv3_se4_2uart_active {
				mux {
					pins = "gpio16", "gpio17";
					function = "qup04";
				};

				config {
						pins = "gpio16", "gpio17";
						drive-strength = <2>;
						bias-disable;
				};
			};

			qupv3_se4_2uart_sleep: qupv3_se4_2uart_sleep {
