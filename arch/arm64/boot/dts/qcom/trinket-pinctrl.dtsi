		/* SE 9 pin mappings */
		qupv3_se9_spi_pins: qupv3_se9_spi_pins {
			qupv3_se9_spi_active: qupv3_se9_spi_active {
				mux {
					pins = "gpio10", "gpio11", "gpio12",
								"gpio13";
					function = "qup_14";
				};

				config {
					pins = "gpio10", "gpio11", "gpio12",
								"gpio13";
