					function = "rgmii_ctl";
				};

				config {
					pins = "gpio74";
					bias-disable;  /* NO pull */
					drive-strength = <2>;
				};
			};
			emac_phy_intr: emac_phy_intr {
				mux {
					pins = "gpio61";
					function = "rgmii_int";
				};

				config {
					pins = "gpio61";
					bias-disable;  /* NO pull */
					drive-strength = <2>;
				};
			};
		};

		evb_tlmm_gpio_key{
			tlmm_gpio_key_active: tlmm_gpio_key_active {
				mux {
					pins = "gpio21","gpio52","gpio54",
						"gpio115";
					function = "gpio";
				};

				config {
					pins = "gpio21","gpio52","gpio54",
						"gpio115";
					drive-strength = <2>;
					bias-pull-up;
				};
			};

			tlmm_gpio_key_suspend: tlmm_gpio_key_suspend {
				mux {
					pins = "gpio21","gpio52","gpio54",
						"gpio115";
					function = "gpio";
				};

				config {
					pins = "gpio21","gpio52","gpio54",
						"gpio115";
					drive-strength = <2>;
					bias-disable;
				};
			};
		};
	};
};
