&tlmm {
	display_panel_avdd_eldo_default: display_panel_avdd_eldo_default {
		mux {
			pins = "gpio130";
			function = "gpio";
		};
		config {
			pins = "gpio130";
			drive-strength = <8>;
			bias-disable = <0>;
			output-high;
		};
	};
};

&sde_dsi {
	vdd-supply = <&display_panel_avdd_eldo>;
};

