#include "sdxprairie-v2.dtsi"
#include "sdxprairie-mtp.dtsi"

&qnand_1 {
	status = "ok";
};

&restart_pshold {
	qcom,force-warm-reboot;
};

/* delete pm8150b nodes */
&soc {
	/delete-node/ thermal-zones;
};

&usb {
	extcon = <&vbus_detect>;
	qcom,smmu-s1-bypass;
};

&spmi_bus {
	/delete-node/ qpnp,fg;
	/delete-node/ bcl@1d00;
	/delete-node/ qcom,usb-pdphy@1700;
	/delete-node/ qcom,qpnp-smb5;
	/delete-node/ adc_tm@3500;
	/delete-node/ vadc@3100;
	/delete-node/ qcom,pm8150b@2;
	/delete-node/ qcom,pm8150b@3;
};

&qnand_1 {
	status = "ok";
};

&blsp1_uart2b_hs {
	status = "ok";
};

&vbus_detect {
	status = "ok";
};

&cnss_qca6390 {
	status = "disabled";
};

&ipa_hw {
	qcom,use-ipa-in-mhi-mode;
};

&pcie0 {
	status = "disabled";
};

&pcie_ep {
	status = "ok";
};

&mhi_device {
	status = "ok";
};

&mhi_net_device {
	status = "ok";
};
