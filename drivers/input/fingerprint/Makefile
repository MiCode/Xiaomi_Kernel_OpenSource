#
# Makefile for the fingerprint drivers.
#

# Each configuration option enables a list of files.

obj-$(CONFIG_FINGERPRINT_XIAOMI_FP)     += xiaomi_fp/
obj-$(CONFIG_FINGERPRINT_GOODIX_FOD)	+= goodix_fod/
obj-$(CONFIG_FINGERPRINT_GOODIX_TA)     += goodix_ta/
obj-$(CONFIG_FINGERPRINT_GOODIX_TEE)    += goodix_tee/
obj-$(CONFIG_FINGERPRINT_FPC)           += fpc/
obj-$(CONFIG_FINGERPRINT_FPC_TEE)       += fpc_tee/
obj-$(CONFIG_FINGERPRINT_GOODIX_FOD_G3)     += goodix_fod_G3/
obj-$(CONFIG_FINGERPRINT_GOODIX_FOD_G3S)     += goodix_fod_G3S/
obj-$(CONFIG_FINGERPRINT_FPC_TA)       += fpc_ta/
