#
# Novatek NT36xxx touchscreen driver configuration
#
config TOUCHSCREEN_NT36532_HOSTDL_SPI
	bool "Novatek NT36532 host download SPI driver"
	default n
	help
	  Say Y here if you have a Novatek NT36532 no flash touchscreen connected
	  to your system by SPI bus.

	  If unsure, say N.

config TOUCHSCREEN_SUPPORT_NEW_GAME_MODE
    tristate "Touch Support New Game Mode"
    default n
    help
      Say Y here if the device support new game mode

      If not support, say N.
